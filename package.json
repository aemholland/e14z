{
  "name": "e14z",
  "version": "4.3.0",
  "description": "Comprehensive Model Context Protocol (MCP) server registry and discovery platform that enables AI agents to find, evaluate, and integrate with MCP tools seamlessly.",
  "main": "bin/cli.js",
  "bin": {
    "e14z": "bin/cli.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:security": "playwright test tests/e2e/security",
    "test:performance": "playwright test tests/e2e/performance",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e -- --reporter=github",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "db:setup": "tsx scripts/apply-database-schema.ts",
    "db:migrate": "tsx scripts/applyCategoryMigration.ts",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker compose up --build",
    "docker:stop": "docker compose down",
    "docker:clean": "docker compose down -v --rmi all",
    "vercel:deploy": "bash scripts/vercel-deploy.sh",
    "vercel:deploy:prod": "bash scripts/vercel-deploy.sh production",
    "vercel:monitor": "bash scripts/vercel-monitor.sh",
    "vercel:monitor:continuous": "bash scripts/vercel-monitor.sh --continuous",
    "vercel:env:setup": "cp .env.production.vercel .env.local && echo 'Please update .env.local with your actual values'",
    "vercel:health": "curl -s https://e14z.com/api/health/vercel | jq",
    "vercel:logs": "vercel logs",
    "postbuild": "echo 'Build completed successfully for Vercel deployment'",
    "crawler": "node bin/crawler.js",
    "crawler:npm": "node bin/crawler.js crawl --source npm",
    "crawler:dry-run": "node bin/crawler.js crawl --dry-run",
    "crawler:test": "node bin/crawler.js test",
    "crawler:update": "node bin/crawler.js update"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "agent",
    "tools",
    "discovery",
    "auto-install",
    "runtime",
    "npx",
    "ai-agents",
    "anthropic",
    "openai",
    "llm",
    "package-manager",
    "security"
  ],
  "author": "E14Z Team",
  "license": "MIT",
  "homepage": "https://e14z.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aemholland/e14z.git"
  },
  "bugs": {
    "url": "https://github.com/aemholland/e14z/issues"
  },
