{
  "name": "e14z",
  "version": "4.1.1",
  "description": "Universal MCP Runtime - NPX-like auto-installation and execution of Model Context Protocol (MCP) servers for AI agents",
  "main": "bin/cli.js",
  "bin": {
    "e14z": "bin/cli.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dedupe": "npx tsx scripts/deduplicate.ts",
    "dedupe:dry": "npx tsx scripts/deduplicate.ts --dry-run",
    "dedupe:auto": "npx tsx scripts/deduplicate.ts --auto-merge",
    "generate-tags": "tsx scripts/generateTags.ts",
    "generate-tags:all": "tsx scripts/generateTags.ts regenerate-all",
    "generate-tags:insufficient": "tsx scripts/generateTags.ts regenerate-insufficient",
    "generate-tags:test": "tsx scripts/generateTags.ts test-generation",
    "crawler": "tsx scripts/crawlerControl.ts",
    "crawler:status": "tsx scripts/crawlerControl.ts status",
    "crawler:test": "tsx scripts/crawlerControl.ts test",
    "crawler:health-check": "tsx scripts/crawlerControl.ts health-check",
    "crawler:history": "tsx scripts/crawlerControl.ts history",
    "clean-formatting": "tsx scripts/cleanDatabaseFormatting.ts",
    "test-installation": "tsx scripts/testInstallationDetection.ts",
    "test-slugs": "tsx scripts/testSlugGeneration.ts",
    "update-slugs": "tsx scripts/updateSlugs.ts",
    "rescrape": "tsx scripts/rescrapeAndValidate.ts",
    "rescrape:test": "tsx scripts/rescrapeAndValidate.ts --test",
    "rescrape:comprehensive": "tsx scripts/comprehensiveRescrape.ts",
    "migrate:categories": "tsx scripts/applyCategoryMigration.ts",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker compose up --build",
    "docker:stop": "docker compose down",
    "docker:clean": "docker compose down -v --rmi all"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "agent",
    "tools",
    "discovery",
    "auto-install",
    "runtime",
    "npx",
    "claude",
    "anthropic",
    "openai",
    "llm",
    "package-manager",
    "security"
  ],
  "author": "E14Z Team",
  "license": "MIT",
  "homepage": "https://e14z.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aemholland/e14z.git"
  },
  "bugs": {
    "url": "https://github.com/aemholland/e14z/issues"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "commonjs",
  "dependencies": {
    "@types/pino": "^7.0.4",
