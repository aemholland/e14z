# Environment variables for E14Z MCP Discovery Platform

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# AI Providers for Tag Generation (at least one required)
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Local LLM (Ollama, LM Studio, etc.)
LOCAL_LLM_URL=http://localhost:11434
LOCAL_LLM_MODEL=llama2

# GitHub API (for enhanced MCP discovery)
GITHUB_TOKEN=your_github_personal_access_token

# Optional: Firecrawl for web scraping
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Application Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=https://e14z.com